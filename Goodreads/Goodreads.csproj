<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.2.0</Version>
    <Authors>Adam Krogh;Vladimir Rybalko</Authors>
    <Product>Goodreads</Product>
    <PackageId>Neyesha.Goodreads</PackageId>
    <Description>Goodreads .NET API Client Library</Description>
    <Copyright>Copyright © 2016</Copyright>
    <RepositoryUrl>https://github.com/Neyesha/goodreads-dotnet</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Neyesha/goodreads-dotnet</PackageProjectUrl>
    <PackageTags>goodreads api netstandard2</PackageTags>
    <CodeAnalysisRuleSet>..\Goodreads.ruleset</CodeAnalysisRuleSet>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\CustomDictionary.xml" Link="CustomDictionary.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="106.2.1" />
  </ItemGroup>
</Project>
